{
  "name": "argster",
  "version": "1.2.2",
  "description": "A simple command/argument manager",
  "main": "lib/index.js",
  "umd:main": "lib/index.umd.js",
  "module": "lib/index.mjs",
  "source": "src/index.ts",
  "types": "lib/index.d.ts",
  "author": "Gudjon Jonsson <gaui@gaui.is> (https://gaui.is)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gaui/argster.git"
  },
  "scripts": {
    "build": "microbundle --target node --external child_process,events,assert,util",
    "prepublishOnly": "yarn build",
    "test": "jest",
    "dts": "tsc --emitDeclarationOnly",
    "lint": "eslint --ext .ts src __tests__ examples",
    "format": "prettier \"./{src,__tests__,examples}/**/*.{js,jsx,ts,tsx,json,md}\" --write",
    "typecheck": "tsc --noEmit --pretty --skipLibCheck"
  },
  "dependencies": {
    "glob": "^7.1.3",
    "ramda": "^0.26.1"
  },
  "browser": {
    "fs": false
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.4.0",
    "@babel/plugin-proposal-class-properties": "7.4.0",
    "@babel/plugin-transform-modules-commonjs": "7.4.0",
    "@babel/preset-env": "7.4.2",
    "@babel/preset-typescript": "7.3.3",
    "@types/glob": "7.1.1",
    "@types/jest": "24.0.11",
    "@types/node": "11.13.17",
    "@types/ramda": "0.26.5",
    "@typescript-eslint/eslint-plugin": "1.5.0",
    "@typescript-eslint/parser": "1.5.0",
    "eslint": "5.16.0",
    "eslint-config-prettier": "4.1.0",
    "eslint-plugin-prettier": "3.0.1",
    "jest": "24.5.0",
    "microbundle": "0.11.0",
    "np": "4.0.2",
    "prettier": "1.16.4",
    "ts-jest": "24.0.1",
    "ts-node": "8.0.3",
    "typescript": "3.4.1"
  }
}
